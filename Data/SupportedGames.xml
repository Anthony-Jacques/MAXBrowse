<supportedgames>
	<game name="Quake 4" serversFile="Q4Servers.xml">
		<installSettings defaultExe="quake4.exe" defaultPath="C:\Program Files\id Software\Quake4\" installRegistrySection="2" installRegistryKey="SOFTWARE\id\Quake 4\" installRegistryValue="InstallPath"/>
		<specialCvars pbcvar="sv_punkbuster" pwcvar="si_usepass" maxplayerscvar="si_maxplayers"/>
		<query masterProtocol="Quake4Master" masterServer="q4master.idsoftware.com" masterPort="27650" serverProtocol="Quake4Server" gamePort="28004"/>
		<columns>
			<column name="Nationality" cvar="nationality" type="0"/>
			<column name="Gametype" cvar="si_gametype" type="0"/>
			<column name="Passworded" cvar="si_usepass" type="1"/> <!-- etqw si_needpass -->
			<column name="Punkbuster" cvar="sv_punkbuster" type="1" />
			<column name="Server IP" cvar="server" type="0"/>
			<column name="Gametype" cvar="si_gametype" type="0"/> <!-- etqw si_rules -->
			<column name="Name" cvar="si_name" type="0"/>
			<column name="Map" cvar="si_map" type="0"/>
			<column name="Mod" cvar="fs_game" type="0"/>
			<column name="Players" cvar="players" type="1"/>
			<column name="Ping" cvar="ping" type="1"/>
			<column name="Buymode" cvar="si_isbuyingenabled" type="1" defaultHidden="1"/> <!-- q4 only -->
			<column name="Mode" cvar="si_mode" type="0" defaultHidden="1"/> <!-- q4only -->
			<column name="Spectators" cvar="si_spectators" type="1" defaultHidden="1"/>
			<column name="SSD" cvar="net_serversnapshotdelay" type="1" defaultHidden="1"/> <!-- q4 only -->
			<column name="FPS" cvar="si_fps" type="1" defaultHidden="1"/> <!-- q4 only -->
			<column name="Version" cvar="si_version" type="0" defaultHidden="1"/> 
			<column name="Modes" cvar="si_modes" type="0" defaultHidden="1"/> <!-- q4 only -->
			<column name="Allow Voting" cvar="si_allowvoting" type="1" defaultHidden="1"/> 
			<column name="Bots Enabled" cvar="bot_enable" type="1" defaultHidden="1"/> <!-- etqw+q4 only -->	
		</columns>
		<playerProperties>
			<property name="Name"/>
			<property name="Clan"/>
			<property name="Ping"/>
		</playerProperties>
	</game>

